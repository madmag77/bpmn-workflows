{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Chainlit",
            "type": "debugpy",
            "request": "launch",
            "program": "chainlit_ui/main.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Run backend",
            "type": "debugpy",
            "request": "launch",
            "program": "backend_run.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Run rename papers workflow",
            "type": "debugpy",
            "request": "launch",
            "program": "awsl/run_awsl_workflow.py",
            "args": [
                "awsl/paper_rename_workflow/paper_rename_workflow.awsl",
                "--functions",
                "awsl.paper_rename_workflow.paper_rename_functions",
                "--param",
                "drafts_folder_path=/Users/artemgoncharov/Downloads/drafts",
                "--param",
                "processed_folder_path=/Users/artemgoncharov/Downloads/processed",
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        }
    ]
}