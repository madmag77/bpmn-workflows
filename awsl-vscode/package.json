{
  "name": "awsl-syntax",
  "displayName": "AWsl Syntax Highlighting",
  "description": "Minimal syntax highlighting for AWsl files",
  "version": "0.0.2",
  "publisher": "local",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "awsl",
        "aliases": [
          "AWsl",
          "awsl"
        ],
        "extensions": [
          ".awsl"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "awsl",
        "scopeName": "source.awsl",
        "path": "./syntaxes/awsl.tmLanguage.json"
      }
    ]
  }
}